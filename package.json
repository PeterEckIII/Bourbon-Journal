{
  "name": "bourbon-journal-8ebc",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "db:prod": "fly ssh console -C database-cli",
    "db:dev": "sqlite3 prisma/data.db",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "dev": "npm run db:migrate && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "docker": "docker compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "precommit": "npm run format && npm run lint && npm run typecheck",
    "pretest:e2e:run": "npm run build",
    "redis": "cd redis/ && fly ssh console -a bourbon-journal-redis",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "run-s db:migrate:test test:unit",
    "test:deploy": "vitest",
    "test:unit": "dotenv -e .env.test -- vitest --no-threads",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run --record --key 1c964687-d43c-4bb5-a6cf-e9a605e2e368\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.6.2",
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@remix-run/server-runtime": "^1.5.1",
    "@remix-validated-form/with-zod": "^2.0.1",
    "@tailwindcss/forms": "^0.5.2",
    "ag-grid-community": "^28.0.2",
    "ag-grid-react": "^28.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.0",
    "core-js": "^3.23.0",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "flowbite": "^1.4.7",
    "react": "^18.1.0",
    "react-collapsed": "^3.3.2",
    "react-dom": "^18.1.0",
    "redis": "^4.1.0",
    "regenerator-runtime": "^0.13.9",
    "remix": "^1.5.1",
    "remix-validated-form": "^4.5.5",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@faker-js/faker": "^6.3.1",
    "@jest/globals": "^28.1.0",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-table": "^7.7.12",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "axe-core": "^4.4.2",
    "babel-loader": "^8.2.5",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "cypress": "^9.6.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^1.7.0",
    "cypress-react-selector": "^3.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.6",
    "happy-dom": "^4.0.1",
    "jest-mock-extended": "^3.0.1",
    "msw": "^0.42.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^3.14.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.9"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
